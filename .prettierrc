{
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "importOrderParserPlugins": ["typescript", "classProperties", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.0.0",
  "importOrder": [
    "^@angular/core/?.*$",
    "^@angular/common/?.*$",
    "^@angular/platform-browser/?.*$",
    "^@angular/router/?.*$",
    "^@angular/forms/?.*$",
    "^@angular/animations/?.*$",
    "^@angular/material/?.*$",
    "^@angular/cdk/?.*$",
    "^@angular/fire/?.*$",
    "",
    "^rxjs.*$",
    "",
    "^@ngx-?.*$",
    "^@ng-?.*$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@app/?.*$",
    "",
    "^.*(\\.module)/?.*$",
    "^.*(\\.config)/?.*$",
    "",
    "^.*(/services|\\.service|/api|\\.api)/?.*$",
    "^.*(/guards|\\.guard)/?.*$",
    "^.*(/components|\\.component|/pages|/modals)/?.*$",
    "^.*(/directives|\\.directive)/?.*$",
    "^.*(/pipes|\\.pipe)/?.*$",
    "^.*(/interfaces|\\.interface|/loaders|\\.loader|/enums|\\.enum|/routes|\\.route|/model|\\.model|/classes)/?.*$",
    "^[./]"
  ]
}
